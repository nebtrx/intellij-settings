<templateSet group="surround">
  <template name="D" value="[$SELECTION$]" shortcut="NONE" description="Surround with []" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="GROOVY" value="false" />
      <option name="JSP" value="false" />
      <option name="SCALA_CODE" value="true" />
      <option name="SCALA_STRING" value="true" />
    </context>
  </template>
  <template name="H" value="$HKT$[$SELECTION$]" shortcut="NONE" description="Surround with higher kinded type []" toReformat="true" toShortenFQNames="true">
    <variable name="HKT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
      <option name="SCALA" value="true" />
    </context>
  </template>
  <template name="Q" value="&quot;$SELECTION$&quot;" shortcut="NONE" description="Surround with &quot;&quot;" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="GROOVY" value="false" />
      <option name="JSP" value="false" />
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="C" value="$TYPE_CONSTRUCTOR$($SELECTION$)" shortcut="NONE" description="Surround with Type Constructor" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE_CONSTRUCTOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="false" />
      <option name="SCALA_CODE" value="true" />
      <option name="SCALA_STRING" value="true" />
    </context>
  </template>
</templateSet>